[0m[27m[24m[J[0m[49m[39m[0m[49m[38;5;31m[1m[38;5;31m[38;5;39m~[0m[38;5;39m[49m[38;5;31m/[1m[38;5;31m[38;5;39mlab5-tasks[0m[38;5;39m[49m[38;5;31m[0m[38;5;31m[49m[38;5;31m[0m[38;5;31m[49m [0m[38;5;31m[49m[38;5;76m❯[0m[38;5;76m[49m[38;5;76m[0m[38;5;76m[49m[30m[0m[30m[49m[39m [0m[49m[39m[K[100C[0m[49m[39m[100D[?1h=[?2004hm[1m[31mm[0m[39m[1m[31mm[0m[39m[90mv order.txt order1.txt[39m[22D[1m[31mm[1m[31mv[0m[39m[0m[32mm[0m[32mv[39m[32mm[32mv[32m [39m[32mm[32mv[39m[39m [39ms[39m [39m [39m [39m [39m [39m [39m [39m [39m [39m [39m [39m [39m [39m [39m [39m [39m [39m [39m [19D[4ms[24m[4ms[4ma[24m[4ma[4mmple.exe[24m[1m [0m[1m [1ms[0m[0m [0m[4ms[24m[4ms[4ma[24m[4ma[4mmple.exe[24m[1m [0m[0m [1C[4m1[4m.[4me[4mx[4me[24m[24ms[24ma[24mm[24mp[24ml[24me[24m1[24m.[24me[24mx[24me[?1l>[?2004l
